 @using System.Web.UI.HtmlControls
 @model SGCorpHR.Models.Response<List<SGCorpHR.Models.Resumes>>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>




    @if (Model.Data != null)
    {
        <table class="table">
            <thead>
                <tr>
                    <th>
                        FileName
                    </th>
                    <th>FilePath</th>
                    <th></th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var file in Model.Data)
                {
                    <tr>
                        <td>@file.FileName</td>
                        <td><a href="@file.FilePath" target="_blank">@file.FilePath</a></td>

                    </tr>
                }
            </tbody>

        </table>
    }
    else
    {
        { <p>@Model.Message</p>}
    }
</div>
