
@{
    ViewBag.Title = "UploadPolicyDoc";
}


@using (Html.BeginForm("SavePolicyDocument", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
{

    <h1>Upload Policy Document</h1>
    <table>
        <tr>
            <td>File:</td>
            <td>
                <input type="file" name="file" />
            </td>
        </tr>
        <tr>
            <td colspan="2">
                <input type="submit" name="Submit" value="Submit" />
            </td>
        </tr>
    </table>
}


